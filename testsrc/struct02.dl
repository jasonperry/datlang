(* mutable struct example, initted by record exp after declaration *)
module Struct02 =

open Stdio;

type Complex = struct
  mut i: Float,
  mut j: Float;
end Complex


export proc main() : Void = 

  var cpx: Complex; (*  = {i=2.0, j=1.111};*)
  cpx = {i=4.0, j=-1.53};
  printFloat(cpx.i);
  printFloat(cpx.j);

end main

end Struct02
